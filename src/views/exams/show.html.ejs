<h1>Exam: <%= exam.name %></h1>


<ul>
<% modules.forEach((module) => { %>
    <li><h3><a href="/exams/<%=exam.id+"/modules/"+module.id%>"><%=module.name%></a></h3></li>
    <% const qua = module.updatedAt %>
    <% var minutes = qua.getMinutes().toString() %>
    <% if (minutes.length == 1) {minutes = "0" + minutes} %>
    <h4>Escrito a las <%= qua.getHours() %>:<%= minutes %> horas el d√≠a <%= qua.getDate() %>/<%= qua.getMonth() + 1 %>/<%= qua.getYear() + 1900%> %> </h4>

    
<% }) %>
</ul>


<br>

<form action="<%= newModulePath %>" method="get">
      <div>

        <input type="submit" name="new" value="New Module" />
      </div>
</form>